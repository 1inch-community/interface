{
  "name": "models",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/models/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "env": {
          "libName": "models",
          "oneModuleLibrary": true
        },
        "commands": [
          "ts-node --project ./tools/packagr/tsconfig.json tools/packagr/main.ts"
        ]
      }
    },
    "watch": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "node tools/esbuild/build.mjs --lib-name models --watch"
        ]
      }
    }
  },
  "tags": []
}
