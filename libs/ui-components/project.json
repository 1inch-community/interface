{
  "name": "ui-components",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/ui-components/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "env": {
          "libName": "ui-components"
        },
        "commands": [
          "ts-node --project ./tools/packagr/tsconfig.json tools/packagr/main.ts"
        ]
      }
    },
    "watch": {
      "executor": "nx:run-commands",
      "options": {
        "env": {
          "libName": "ui-components",
          "isWatch": true
        },
        "commands": [
          "ts-node --project ./tools/packagr/tsconfig.json tools/packagr/main.ts"
        ]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "../../coverage/temp/ui-components"
      }
    }
  },
  "tags": []
}
