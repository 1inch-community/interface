{
  "name": "sdk",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/sdk/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "rollup -c libs/sdk/rollup.config.js --bundleConfigAsCjs"
        ]
      }
    },
    "watch": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "node tools/esbuild/build.mjs --lib-name sdk --watch"
        ]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "../../coverage/temp/ui-components"
      }
    }
  },
  "tags": []
}
