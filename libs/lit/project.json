{
  "name": "lit",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/lit/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "env": {
          "libName": "lit",
          "oneModuleLibrary": true
        },
        "commands": [
          "pnpm run build:lib"
        ]
      }
    },
    "watch": {
      "executor": "nx:run-commands",
      "options": {
        "env": {
          "libName": "lit",
          "oneModuleLibrary": true,
          "isWatch": true
        },
        "commands": [
          "pnpm run build:lib"
        ]
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "tsc --project libs/lit/tsconfig.json --noEmit --skipLibCheck"
        ]
      }
    }
  },
  "tags": []
}
